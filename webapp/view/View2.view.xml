    <mvc:View
        controllerName="customer.com.ui5app.controller.View2"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns="sap.m"
        xmlns:f="sap.f">

        <f:DynamicPage id="_IDGenDynamicPage1" fitContent="true">

            <!-- Header -->
            <f:title>
                <f:DynamicPageTitle id="_IDGenDynamicPageTitle1">

                    

                    <!-- Title Section -->
                    <f:heading>
                        <VBox id="_IDGenVBox">
                            <Button id="_IDGenButton1"
                            icon="sap-icon://nav-back"
                            type="Transparent"
                            press="onNavBack"/>
                            
                            <Title id="_IDGenTitle2"
                                text="{Title} {FirstName} {LastName}"
                                level="H2"/>
                            <Text id="_IDGenText8" text="{City}" />
                        </VBox>
                    </f:heading>

                </f:DynamicPageTitle>
            </f:title>

            <!-- Content -->
            <f:content>
                <Table id="_IDGenTable"
                    inset="false"
                    showSeparators="Inner"
                    width="100%">

                    <columns>
                        <Column id="_IDGenColumn4" width="40%">
                            <Text id="_IDGenText9" text=""/>
                        </Column>
                        <Column id="_IDGenColumn5" width="60%">
                            <Text id="_IDGenText10" text=""/>
                        </Column>
                    </columns>

                    <items>
                        <ColumnListItem id="_IDGenColumnListItem2">
                            <cells>
                                <Text id="_IDGenText11" text="Register Date" />
                                <Text id="_IDGenText12" text="{RegisterDate}"/>
                            </cells>
                        </ColumnListItem>

                        <ColumnListItem id="_IDGenColumnListItem3">
                            <cells>
                                <Text id="_IDGenText13" text="Address" />
                                <Text id="_IDGenText14" text="{Address}"/>
                            </cells>
                        </ColumnListItem>

                        <ColumnListItem id="_IDGenColumnListItem4">
                            <cells>
                                <Text id="_IDGenText15" text="Notes" />
                                <Text id="_IDGenText16" text="{Notes}"/>
                            </cells>
                        </ColumnListItem>

                    </items>

                </Table>
            </f:content>

        </f:DynamicPage>

    </mvc:View>